	<!--日志消息发送 -->
	<bean id="logMessageSender" class="com.zengshi.paas.client.message.MessageSender"
		init-method="init">
		<property name="cc" ref="cc" />
		<property name="confPath" value="/com/zengshi/paas/message/logMessageSender" />
	</bean>
	<!--通用消息发送 -->
	<bean id="messageSender" class="com.zengshi.paas.client.message.MessageSender"
		init-method="init">
		<property name="cc" ref="cc" />
		<property name="confPath" value="/com/zengshi/paas/message/messageSender" />
	</bean>

	<!--日志信息写入 mongodb -->
	<bean id="logWriter" class="com.zengshi.paas.client.log.MongoLogWriter"
		init-method="init">
		<property name="cc" ref="cc" />
		<property name="confPath" value="/com/zengshi/paas/logger/conf" />
	</bean>
	<!--跟踪日志信息写入 mongodb -->
	<bean id="tradeLogWriter" class="com.zengshi.paas.client.log.MongoLogWriter"
		init-method="init">
		<property name="cc" ref="cc" />
		<property name="confPath" value="/com/zengshi/paas/logger/tradeLogWriter" />
	</bean>
	<!--文件信息写入 mongodb -->
	<bean id="fileManager" class="com.zengshi.paas.client.file.MongoFileManager"
		init-method="init">
		<property name="cc" ref="cc" />
		<property name="confPath" value="/com/zengshi/paas/file/conf" />
	</bean>
	<!--redis cache -->
	<bean id="cacheSvc" class="com.zengshi.paas.client.cache.CacheSvcImpl"
		init-method="init">
		<property name="cc" ref="cc" />
		<property name="confPath" value="/com/zengshi/paas/cache/conf" />
	</bean>
